{
  "name": "$datasetName-s3fs-container",
  "containerImage": "$containerRegistryUrl/s3fs-launcher@$digest",
  "command": [
    "python3.10",
    "./s3fs-launcher.py",
    "$readOnly",
    "--mount-path",
    "$mountPath/$datasetName"
  ],
  "environmentVariables": [
    {
      "name": "AWS_BUCKET_NAME",
      "value": "$awsBucketName",
      "strategy": "string"
    },
    {
      "name": "ACCESS_NAME",
      "value": "$datasetName",
      "strategy": "string"
    },
    {
      "name": "CGS_AWS_S3_CONFIG_SECRET",
      "value": "$cgsAwsS3ConfigSecret",
      "strategy": "string"
    },
    {
      "name": "OTEL_EXPORTER_OTLP_ENDPOINT",
      "value": ".*",
      "strategy": "re2"
    },
    {
      "name": "OTEL_EXPORTER_OTLP_PROTOCOL",
      "value": ".*",
      "strategy": "re2"
    },
    {
      "name": "OTEL_TRACE_CONTEXT_BASE64",
      "value": ".*",
      "strategy": "re2"
    },
    {
      "name": "TELEMETRY_MOUNT_PATH",
      "value": "$telemetryMountPath",
      "strategy": "string"
    },
    {
      "name": "VOLUMESTATUS_MOUNT_PATH",
      "value": "$volumeStatusMountPath",
      "strategy": "string"
    }
  ],
  "mounts": [
    {
      "name": "remotemounts",
      "mountPath": "$mountPath",
      "mountType": "emptyDir",
      "readOnly": false
    },
    {
      "name": "telemetrymounts",
      "mountPath": "$telemetryMountPath",
      "mountType": "emptyDir",
      "readOnly": false
    },
    {
      "name": "volumestatusmounts",
      "mountPath": "$volumeStatusMountPath",
      "mountType": "emptyDir",
      "readOnly": false
    }
  ],
  "securityContext": {
    "privileged": "true"
  }
}